journal = {Psychology and Aging},
keywords = {2002,age,and physical competence decline,cognition,cognitive,cuddy,fiske,memory,negative stereotypes that memory,older adults face pervasive,performance,stereotype threat,these stereotypes make them,vulnerable to age,with age},
number = {1},
pages = {180--193},
pmid = {25621742},
title = {{A review and meta-analysis of age-based stereotype threat: Negative stereotypes, not facts, do the damage}},
url = {http://psycnet.apa.org/journals/pag/30/1/180.pdf},
volume = {30},
