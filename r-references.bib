file = {:Users/cameronkay/Documents/Master References Organized/2018 - Lester et al. - Epigenetic programming by maternal behavior in the human infant.pdf:pdf},
isbn = {0000000000},
issn = {0031-4005},
journal = {Pediatrics},
number = {4},
pages = {e20171890},
pmid = {30257918},
title = {{Epigenetic programming by maternal behavior in the human infant}},
url = {http://pediatrics.aappublications.org/lookup/doi/10.1542/peds.2017-1890},
volume = {142},
year = {2018}
