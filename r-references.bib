pages = {242--249},
publisher = {Elsevier Ltd},
title = {{Sexually violent women: The PID-5, everyday sadism, and adversarial sexual attitudes predict female sexual aggression and coercion against male victims}},
url = {http://dx.doi.org/10.1016/j.paid.2017.02.019},
volume = {111},
