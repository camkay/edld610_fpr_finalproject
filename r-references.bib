journal = {Personality and Individual Differences},
keywords = {Dark Triad,Five factor model,Happiness,Latent profile analysis,Machiavellianism,Narcissism,Psychopathy,Subjective well-being},
pages = {17--22},
publisher = {Elsevier Ltd},
title = {{The Dark Triad, happiness and subjective well-being}},
url = {http://dx.doi.org/10.1016/j.paid.2014.01.004},
volume = {67},
year = {2014}
}
