@article{Mullins-Sweatt2010,
abstract = {There has long been interest in identifying and studying " successful psychopaths." This study sampled psychologists with an interest in law, attorneys, and clinical psychology professors to obtain descriptions of individuals considered to be psychopaths who were also successful in their endeavors. The results showed a consistent description across professions and convergence with descriptions of traditional psychopathy, though the successful psychopathy profile had higher scores on conscientiousness, as measured within the five-factor model (FFM). These results are useful in documenting the existence of successful psychopathy, demonstrating the potential benefit of informant methodology, and providing an FFM description that distinguishes successful psychopaths from unsuccessful psychopaths studied more routinely within prison settings. {\textcopyright} 2010 Elsevier Inc.},
