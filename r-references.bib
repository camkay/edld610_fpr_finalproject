isbn = {0191-8869},
issn = {01918869},
journal = {Personality and Individual Differences},
keywords = {Antisocial Internet behavior,Cyber-trolls,Dark Tetrad,Dark Triad,Personality,Sadism,Trolling},
pages = {97--102},
