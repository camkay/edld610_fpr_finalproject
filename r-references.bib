isbn = {1091-6490 (Electronic)$\backslash$r0027-8424 (Linking)},
issn = {0027-8424},
journal = {Proceedings of the National Academy of Sciences},
number = {12},
pages = {201420870},
pmid = {25775577},
title = {{Origins of narcissism in children}},
url = {http://www.pnas.org/lookup/doi/10.1073/pnas.1420870112},
volume = {112},
year = {2015}
}
