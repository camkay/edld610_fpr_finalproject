isbn = {0165-1781},
issn = {09254927},
journal = {Psychiatry Research: Neuroimaging},
keywords = {ADHD,Default-mode network,Development,Dorsal anterior cingulate cortex,Functional connectivity,Resting-state fMRI},
number = {2},
pages = {120--127},
pmid = {22424873},
title = {{Abnormal functional connectivity between the anterior cingulate and the default mode network in drug-na{\"{i}}ve boys with attention deficit hyperactivity disorder}},
