file = {:Users/cameronkay/Documents/Master References Organized/2008 - M{\"{u}}ller et al. - Gray matter changes in right superior temporal gyrus in criminal psychopaths. Evidence from voxel-based morphome.pdf:pdf},
issn = {09254927},
journal = {Psychiatry Research: Neuroimaging},
keywords = {Brain structure,Frontotemporal network,PCL-R,Pathogenesis,Psychopathy,Superior temporal gyrus,Volume loss,Voxel-based morphometry},
