@article{Gleichgerrcht2013,
abstract = {Is it permissible to harm one to save many? Classic moral$\backslash$ndilemmas are often defined by the conflict between a$\backslash$nputatively rational response to maximize aggregate welfare$\backslash$n(i.e., the utilitarian judgment) and an emotional aversion to$\backslash$nharm (i.e., the non-utilitarian judgment). Here, we address$\backslash$ntwo questions. First, what specific aspect of emotional$\backslash$nresponding is relevant for these judgments? Second, is this$\backslash$naspect of emotional responding selectively reduced in$\backslash$nutilitarians or enhanced in non-utilitarians? The results$\backslash$nreveal a key relationship between moral judgment and empathic$\backslash$nconcern in particular (i.e., feelings of warmth and compassion$\backslash$nin response to someone in distress). Utilitarian participants$\backslash$nshowed significantly reduced empathic concern on an$\backslash$nindependent empathy measure. These findings therefore reveal$\backslash$ndiminished empathic concern in utilitarian moral judges.},
author = {Gleichgerrcht, Ezequiel and Young, Liane L.},
