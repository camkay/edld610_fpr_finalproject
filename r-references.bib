@article{Yildirim2016,
abstract = {Since its inception, the concept of “secondary psychopathy” has been contrasted with primary psychopathy on the grounds of neuroticism, affect instability, and anxiety. Nonetheless, while the etiology of primary psychopathy, and its various possible expressions, has received ample attention, secondary psychopathy has been largely neglected and only discussed in cluster-analytic studies. This paper seeks to fill that caveat by delineating the continuum on which secondary psychopathic individuals may be set apart and exploring potential sources of within group homogeneity and heterogeneity. While secondary psychopathic individuals share a number of etiological antecedents (attachment problems, childhood maltreatment, neurobiological dysfunctions) they can vary on different temperamental predispositions and specific types of maltreatment experienced in childhood (neglect/abuse vs. trauma/abuse, low vs. high prefrontal catecholaminergic activity), thus creating the heterogeneity observed in this group. Secondary psychopathic individuals may be situated on a continuum that spans from attentional hyposensitivity and cognitive distractibility on the far left side, associated with deficient prefrontal catecholamine activity and childhood neglect/abuse, ultimately culminating in more detached expressions of secondary psychopathy (callousness, hedonism, and low worrying), to attentional hypersensitivity and cognitive rigidity on the far right side, associated with non-optimally high levels of catecholamine activity and childhood relational trauma/abuse leading to more unstable expressions of secondary psychopathy (hostility, neuroticism, instability, and worrying).},
author = {Yildirim, Bariş O.},
doi = {10.1016/j.avb.2016.09.004},
file = {:Users/cameronkay/Documents/Master References Organized/2016 - Yildirim - A treatise on secondary psychopathy Psychobiological pathways to severe antisociality.pdf:pdf},
isbn = {1359-1789},
issn = {18736335},
journal = {Aggression and Violent Behavior},
keywords = {Antisocial personality,Borderline personality,Dopamine,Gender differences,Prefrontal cortex,Psychopathy subtypes,Serotonin},
pages = {165--185},
pmid = {613540659},
title = {{A treatise on secondary psychopathy: Psychobiological pathways to severe antisociality}},
volume = {31},
year = {2016}
}
