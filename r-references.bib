file = {:Users/cameronkay/Documents/Master References Organized/2015 - O'Boyle et al. - A Meta-Analytic Test of Redundancy and Relative Importance of the Dark Triad and Five-Factor Model of Personalit.pdf:pdf},
isbn = {1467-6494},
