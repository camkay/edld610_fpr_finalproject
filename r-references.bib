file = {:Users/cameronkay/Documents/Master References Organized/1982 - Holmes, Sherman - The Ontogeny of Kin Recognition in Two Species of Ground Squirrels The Ontogeny of Kin Recognition in Two Speci.pdf:pdf},
journal = {Source: American Zoologist Amer. Zool},
number = {22},
pages = {491--517},
title = {{The Ontogeny of Kin Recognition in Two Species of Ground Squirrels The Ontogeny of Kin Recognition in Two Species of Ground Squirrels1}},
url = {http://www.jstor.org/stable/3882574{\%}5Cnhttp://www.jstor.org/{\%}5Cnhttp://www.jstor.org/action/showPublisher?publisherCode=oup.{\%}5Cnhttp://www.jstor.org},
volume = {22},
