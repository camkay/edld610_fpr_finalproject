doi = {10.1016/j.paid.2018.02.023},
file = {:Users/cameronkay/Documents/Master References Organized/2018 - Alavi, Kye Mei, Mehrinezhad - The Dark Triad of personality and infidelity intentions The moderating role of relationship experie.pdf:pdf},
