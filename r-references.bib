journal = {Frontiers in Psychology},
keywords = {Dark personality,Explicit images,Mate value,Online dating,Short-term mating},
number = {DEC},
pages = {1--6},
pmid = {29326632},
title = {{Sending nudes: Sex, self-rated mate value, and trait Machiavellianism predict sending unsolicited explicit images}},
volume = {8},
year = {2017}
}
