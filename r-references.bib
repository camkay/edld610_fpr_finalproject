volume = {55},
year = {2013}
}
@article{Buss2013,
abstract = {Comments on an article by Steve Stewart-Williams and Andrew G. Thomas (see record 2013-31861-001). Stewart-Williams and Thomas contrasted two labels to capture the essence of human mating-the males compete/females choose (MCFC) model and the mutual mate choice (MMC) model. The original authors preferred the label of MMC and they emphasize long-term committed mating, and that's perfectly fine. MMC, male and female mate preferences, female and male mate competition, and long-term committed mating are, and have been, central to work on human mating strategies for the past few decades. The present author prefers a broader label, such as human mating strategies or sexual strategies theory, phrases that are admittedly more diffuse but nonetheless do not preclude the multiple mating strategies known to exist in some cases, and hypothesized to exist in other cases-mating phenomena not well captured by the MMC label. The key point is that the importance of a sex difference is properly gauged not solely by the magnitude of effect. Other metrics of "importance" include fitness consequences, physical costs, and psychological costs. In the end, commentator think, it prudent not to conclude that sex differences in sexual psychology are unimportant. (PsycINFO Database Record (c) 2013 APA, all rights reserved)},
author = {Buss, David M.},
doi = {10.1080/1047840X.2013.819552},
file = {:Users/cameronkay/Documents/Master References Organized/2013 - Buss - The Science of Human Mating Strategies An Historical Perspective.pdf:pdf},
isbn = {1047-840X},
issn = {1047840X},
journal = {Psychological Inquiry},
number = {3},
pages = {171--177},
title = {{The Science of Human Mating Strategies: An Historical Perspective}},
volume = {24},
year = {2013}
}
