journal = {Journal of Economic Psychology},
keywords = {Career,Fearless dominance,Professional,Psychopathy,Self-centered impulsivity},
pages = {130--139},
publisher = {Elsevier B.V.},
title = {{Do psychopathic traits predict professional success?}},
